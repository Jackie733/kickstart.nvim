{
  "Console log": {
    "prefix": ["clg", "log"],
    "description": "Console log",
    "body": ["console.log($0)"]
  },
  "Arrow function": {
    "prefix": ["af", "arrow"],
    "description": "Arrow function",
    "body": ["const ${1:name} = (${2:params}) => {", "  $0", "}"]
  },
  "Async arrow function": {
    "prefix": ["aaf", "asyncarrow"],
    "description": "Async arrow function",
    "body": ["const ${1:name} = async (${2:params}) => {", "  $0", "}"]
  }
}
